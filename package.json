{
  "name": "react-native-template-tamagui-cognito",
  "version": "1.0.0",
  "description": "A React Native template with Expo, Tamagui, and AWS Cognito authentication",
  "main": "index.ts",
  "scripts": {
    "start": "expo start --dev-client",
    "start:clear": "expo start --clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "build:web": "expo build:web",
    "eject": "expo eject",
    "lint": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "prebuild": "expo prebuild",
    "clean": "expo install --fix"
  },
  "dependencies": {
    "@aws-amplify/react-native": "^1.1.10",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/native": "^7.1.12",
    "@react-navigation/native-stack": "^7.3.17",
    "@tamagui/babel-plugin": "^1.127.0",
    "@tamagui/config": "^1.127.0",
    "@tamagui/focusable": "^1.127.0",
    "@tamagui/font-inter": "^1.127.0",
    "@tamagui/get-button-sized": "^1.127.0",
    "@tamagui/react-native-media-driver": "^1.127.0",
    "aws-amplify": "^6.15.1",
    "burnt": "^0.13.0",
    "expo": "~53.0.10",
    "expo-constants": "^17.1.6",
    "expo-dev-client": "~5.2.1",
    "expo-secure-store": "^14.2.3",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
